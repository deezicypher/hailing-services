{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/hailing-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/hailing-services/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/hailing-services",
  "projects": {
    "hailing-services": {
      "type": "application",
      "root": "apps/hailing-services",
      "entryFile": "main",
      "sourceRoot": "apps/hailing-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hailing-services/tsconfig.app.json"
      }
    },
    "logging": {
      "type": "application",
      "root": "apps/logging",
      "entryFile": "main",
      "sourceRoot": "apps/logging/src",
      "compilerOptions": {
        "tsConfigPath": "apps/logging/tsconfig.app.json"
      }
    },
    "rider-service": {
      "type": "application",
      "root": "apps/rider-service",
      "entryFile": "main",
      "sourceRoot": "apps/rider-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rider-service/tsconfig.app.json"
      }
    }
  }
}